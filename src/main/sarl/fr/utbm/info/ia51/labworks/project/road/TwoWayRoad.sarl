package fr.utbm.info.ia51.labworks.project.road

import java.util.UUID
import fr.utbm.info.ia51.labworks.project.events.EnterTwoWayRoad

class TwoWayRoad extends Road{
	public var nbOfLanePrimary : int
	public var nbOfLaneSecondary : int
	
	def enter(car : UUID) {
		super.enter(car)
		this.context.getDefaultSpace().emit(this.roadElementUUID, new EnterTwoWayRoad(this))[it.UUID == car]
	}
}