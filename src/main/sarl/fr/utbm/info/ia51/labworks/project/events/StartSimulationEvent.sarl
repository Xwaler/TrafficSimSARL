package fr.utbm.info.ia51.labworks.project.events

import io.sarl.lang.core.Event

event StartSimulationEvent {
	var nbCars : int
	var autonomousProportions : double
	var obedienceRate : double
	
	new (nbCars : int, autonomousProportions : double, obedienceRate : double) {
		this.nbCars = nbCars
		this.autonomousProportions = autonomousProportions
		this.obedienceRate = obedienceRate
	}
}
