package fr.utbm.info.ia51.labworks.project.environment.^agent

import java.util.UUID

capacity RoadManager {
	def createCar() : CarBody
}

skill RoadManagerSkill implements RoadManager {
	def createCar() : CarBody {
		var agentId : UUID = UUID::randomUUID
		var constructor = CarBody.getDeclaredConstructor(typeof(UUID))
		return constructor.newInstance(agentId)
	}
}
