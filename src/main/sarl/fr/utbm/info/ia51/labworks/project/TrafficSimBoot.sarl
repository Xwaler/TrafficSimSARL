package fr.utbm.info.ia51.labworks.project


import fr.utbm.info.ia51.labworks.project.ui.MainFrame
import io.sarl.core.Initialize
import io.sarl.core.Lifecycle
import io.sarl.core.DefaultContextInteractions
import fr.utbm.info.ia51.labworks.project.environment.Environment

agent TrafficSimBoot{
	uses Lifecycle, DefaultContextInteractions

	var nbCar : int = 10
	
	on Initialize {
		var ui = new MainFrame(defaultContext);
		spawn(typeof(Environment), nbCar, ui)
	}
}
